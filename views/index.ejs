<%- include('partials/header.ejs'); -%>
<div class="box" id="heading">
  <h1>Lists</h1>
</div>
<ul class="lists">
  <% lists.forEach((list)=>{ %>
  <li class="box" id="heading">
    <h1><a href="/<%= list.id %>"><%= list.name %></a></h1>
  </li>

  <%}) %>
</ul>

<button class="bottom"><a href="/addListForm">+</a></button>
